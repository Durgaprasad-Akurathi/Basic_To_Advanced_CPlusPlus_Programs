// --------------> Using For Loop <-------------- //

#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    for(int i=1; i<=10; i++)
    cout<<n<<"*"<<i<<"="<<n*i<<endl;
}

-------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------


// --------------> Using While Loop <-------------- //

#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n, i=1;
    cin>>n;
    while(i<=10)
    {
        cout<<n<<"*"<<i<<"="<<n*i<<endl;
        i++;
    }
}

-------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------


// --------------> Using Do While Loop <-------------- //

#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n, i=1;
    cin>>n;
    do
    {
        cout<<n<<"*"<<i<<"="<<n*i<<endl;
        i++;
    }while(i<=10);
}

-------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
