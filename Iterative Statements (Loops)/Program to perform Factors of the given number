// --------------> Using For Loop <-------------- //

#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    for(int i=1; i<n; i++)
    {
        if(n%i==0)
        cout<<i<<" ";
    }
}

-------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------


// --------------> Using While Loop <-------------- //

#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,i=1;
    cin>>n;
    while(i<n)
    {
        if(n%i==0)
        cout<<i<<" ";
        i++;
    }
}

-------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------


// --------------> Using Do While Loop <-------------- //

#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,i=1;
    cin>>n;
    do
    {
        if(n%i==0)
        cout<<i<<" ";
        i++;
    }while(i<n);
}

-------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
