#include<bits/stdc++.h>
using namespace std;
int main()
{
    int r,c, sum=0;
    cin>>r>>c;
    int a[r][c];
    for(int i=0; i<r; i++)
    for(int j=0; j<c; j++)
    cin>>a[i][j];
    for(int i=0; i<r; i++)
    {
        for(int j=0; j<c; j++)
        {
            if(j==i)
            sum+=a[j][i];
        }
    }
    cout<<"Right Diagonal Sum: "<<sum<<endl;
}
