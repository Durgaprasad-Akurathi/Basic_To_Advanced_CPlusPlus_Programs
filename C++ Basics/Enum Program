#include<bits/stdc++.h>
using namespace std;
enum days{mon,tue,wed,thu,fri,sat,sun};
int main()
{
    days d;
    cout<<mon<<endl;
    cout<<tue<<endl;
    cout<<wed<<endl;
    cout<<thu<<endl;
    cout<<fri<<endl;
    cout<<sat<<endl;
    cout<<sun<<endl;
}


