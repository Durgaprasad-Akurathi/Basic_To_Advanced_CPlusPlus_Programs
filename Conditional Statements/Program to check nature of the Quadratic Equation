#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a,b,c,d,r1,r2;
    cin>>a>>b>>c;
    r1=(-b+(sqrt((b*b)-2*a*c)))/2*a;
    r1=(-b-(sqrt((b*b)-2*a*c)))/2*a;
    cout<<r1<<endl;
    cout<<r2<<endl;
    if(((b*b)-2*a*c)==0)
    cout<<"Roots are REAL & EQUAL";
    else if(((b*b)-2*a*c)>0)
    cout<<"Roots are REAL & UNEQUAL";
    else cout<<"Roots are IMAGINARY";
}
