#include<bits/stdc++.h>
using namespace std;
int main()
{
    int m1, m2, m3, m4, m5, m6;
    cout<<"please enter the marks in between 0 to 100 for all 6 subjects: ";
    cin>>m1>>m2>>m3>>m4>>m5>>m6;
    int result=m1+m2+m3+m4+m5+m6;
    cout<<"Total Result: "<<result<<endl;
    if(result>=500 && result<=600)
    cout<<"A+";
    else if(result>=400 && result<=599)
    cout<<"A";
    else if(result>=350 && result<=399)
    cout<<"B+";
    else if(result>=270 && result<=349)
    cout<<"B";
    else if(result>=200 && result<=269)
    cout<<"C";
    else if(result>=150 && result<=199)
    cout<<"D";
    else if(result>=100 && result<=149)
    cout<<"E";
    else cout<<"Fail";
}
