#include<bits/stdc++.h>
using namespace std;
int main()
{
    int start_time, end_time;
    cout<<"Please enter the valid numbers in 24hrs format: ";
    cin>>start_time>>end_time;
    if(end_time<=24)
    {
        if(start_time>=9 && end_time<=18)
        cout<<"Working Hours";
        else cout<<"Non Working hours";    
    }
    else cout<<"Please enter the valid time in 24hrs format";
}
